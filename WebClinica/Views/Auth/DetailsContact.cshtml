@model WebClinica.Models.TbContacto

@{
    ViewBag.Title = "DetailsContact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalle del Mensaje</h2>

<div>
 
    <dl class="dl-horizontal">
        <dt> Nombre Completo </dt>
        <dd> @Html.DisplayFor(model => model.nom_cont) </dd>

        <dt> E-mail </dt>
        <dd> @Html.DisplayFor(model => model.emi_cont) </dd>

        <dt> Telefono </dt>
        <dd> @Html.DisplayFor(model => model.tel_cont) </dd>

        <dt>Asunto </dt>
        <dd> @Html.DisplayFor(model => model.asu_cont) </dd>

        <dt> Mensaje</dt> 
        <dd> @Html.DisplayFor(model => model.men_cont) </dd>

        <dt> Fecha Registro </dt>
        <dd>  @Html.DisplayFor(model => model.fecreg) </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Regrasar a la Lista", "Index")
</p>
